$(document).ready(function){
    $('div').fadeIn('slow');
});

**##6. Selecting by Class**
$(document).ready(function() {
    $('button').click(function() {
       $('.vanish').fadeOut('slow') 
    });
});

**##7. Selecting by ID**
$(document).ready(function() {
    $('button').click(function() {
        $('#blue').fadeOut('slow');
    });
});

**## 8.Flexible Selections**
$(document).ready(function(){
   $('.pink,.red').fadeTo('slow',0);
  
});

**##9. 'this' is Important!**
$(document).ready(function() {
    $('div').click(function() {
        $(this).fadeOut('slow');
    });
});

